{
  "name": "tsmotta/yii2-oauth2-rest-template",
  "description": "Yii2 Rest APi Template with OAuth2 (fully configured filsh/yii2-oauth2-server extension)",
  "keywords": [
    "yii2",
    "rest",
    "oauth2",
    "application template",
    "api"
  ],
  "homepage": "https://github.com/tsmotta/yii2-oauth2-rest-template",
  "type": "project",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Thiago S Motta",
      "email": "tsmotta@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/tsmotta/yii2-oauth2-rest-template/issues",
    "source": "https://github.com/tsmotta/yii2-oauth2-rest-template"
  },
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "~2.0.14",
    "hosannahighertech/yii2-oauth2-server": "1.0.*"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.0.0",
    "yiisoft/yii2-gii": "~2.0.0",
    "yiisoft/yii2-faker": "~2.0.0",
    "codeception/base": "^2.2.3",
    "codeception/verify": "~0.3.1",
    "codeception/specify": "~0.4.3"
  },
  "extra": {
    "writable": [
      "api/runtime"
    ],
    "executable": [
      "api/yii"
    ]
  }
}
